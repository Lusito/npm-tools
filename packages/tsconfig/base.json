{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["ES2015"],
    "declaration": true,
    "moduleResolution": "node",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitOverride": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true
  }
}
